DataBase1 <- DataBase1 %>% slice(-c(125))
toupper(DataBase1$Universidad) -> DataBase1$Universidad
DataBase1$Universidad[DataBase1$Universidad == "UDEP"] <- "UNIVERSIDAD DE PIURA."
DataBase1$Universidad[DataBase1$Universidad == "ULIMA"] <- "UNIVERSIDAD DE LIMA."
DataBase1$Universidad[DataBase1$Universidad == "UCSUR"] <- "UNIVERSIDAD CIENTIFICA DEL SUR."
DataBase1$Universidad[DataBase1$Universidad == "UNI" | DataBase1$Universidad == "UNIVERSIDAD NACIONAL DE INGENIERÍA." | DataBase1$Universidad == "UNIVERSIDAD NACIONAL DE INGENIERIA" | DataBase1$Universidad == "UNIVERSIDAD NACIONAL DE INGENERIA"] <- "UNIVERSIDAD NACIONAL DE INGENIERIA."
DataBase1$Universidad[DataBase1$Universidad == "UDEP"] <- "UNIVERSIDAD DE PIUR.A"
DataBase1$Universidad[DataBase1$Universidad == "UPC" | DataBase1$Universidad == "UNIVERSIDAD PERUANA DE CIENCIAS APLICADAS (UPC)" | DataBase1$Universidad == "UPC - UNIVERSIDAD PERUANA DE CIENCIAS APLICADAS."] <- "UNIVERSIDAD PERUANA DE CIENCIAS APLICADAS."
DataBase1$Universidad[DataBase1$Universidad == "UNIVERSIDAD DE INGENIERÍA Y TECNOLOGÍA." | DataBase1$Universidad == "UNIVERSIDAD DE INGENIERíA  Y TECNOLOGíA." | DataBase1$Universidad == "UNIVERSIDAD DE INGENIERÍA Y TECNOLOGÍA."] <- "UTEC"
