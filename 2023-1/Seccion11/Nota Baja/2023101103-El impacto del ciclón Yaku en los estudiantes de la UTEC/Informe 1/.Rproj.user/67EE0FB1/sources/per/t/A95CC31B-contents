PCA4H <- (nrow(DM[DM$`Cantidad a.fisica` == "AF4",])/200)/PH
PCA5H <- (nrow(DM[DM$`Cantidad a.fisica` == "AF5",])/200)/PH
PCA6H <- (nrow(DM[DM$`Cantidad a.fisica` == "AF6",])/200)/PH
