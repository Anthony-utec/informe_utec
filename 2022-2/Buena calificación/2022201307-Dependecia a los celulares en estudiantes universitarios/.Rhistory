theme_bw()+
geom_segment(size=8) +
labs(title='Planificación PG2', x='Tiempo (Semanas)', y='Actividades') +
scale_colour_manual(values = c('#F43E17', '#7FF417','#F4E217', '#F117F4','#663399','#3333FF'))
