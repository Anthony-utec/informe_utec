PrePromProdMes = function(DF,mes){
DFAux   = filter(DF,DF$Mes == mes)
DFAux$PreDia <- rowMeans(DFAux[,c("PrePromAyer","PrePromHoy")],na.rm=TRUE)
PrePromMes  = c(
round(median(DFAux[which(DFAux$Producto == "Uva Alfonso Lavalett"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Arra"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Italia"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Magenta"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Palestina"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Pirovano"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Quebranta/Moscatel"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Red Globe"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Rosada"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Sultanina"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Superior"),]$PreDia,na.rm=TRUE),3),
round(median(DFAux[which(DFAux$Producto == "Uva Zafiro"),]$PreDia,na.rm=TRUE),3))
return(
PrePromMes
)
}
